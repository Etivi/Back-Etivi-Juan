{
  "name": "etivi-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:local": "dotenv -e .env.local  nest build",
    "build:remote": "dotenv -e .env.remote  nest build",
    "build:prod": "dotenv -e .env.prod  nest build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main.js",
    "start:dev": "nest start --watch",
    "start:dev:remote": "dotenv -e .env.remote nest start --watch",
    "start:dev:prod": "dotenv -e .env.prod nest start --watch",
    "start:prod": "dotenv -e .env.prod node dist/src/main.js",
    "start:remote": "dotenv -e .env.remote node dist/src/main.js",
    "dev": "cross-env NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker-db:up": "docker compose --file ./docker-compose-databases-only.yml up",
    "docker-db:down": "docker compose --file ./docker-compose-databases-only.yml down",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/websockets": "^9.4.0",
    "@prisma/client": "4.9.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/passport-jwt": "^3.0.8",
    "axios": "^1.3.2",
    "bcrypt": "^5.1.0",
    "cache-manager": "^4.0.0",
    "cache-manager-redis-store": "^2.0.0",
    "cron": "^2.3.0",
    "cross-env": "^7.0.3",
    "datastructures-js": "^12.0.0",
    "dotenv-cli": "^7.0.0",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "gaxios": "^6.1.1",
    "google-auth-library": "^8.7.0",
    "googleapis": "^110.0.0",
    "jsonwebtoken": "^9.0.0",
    "mime-types": "^2.1.35",
    "nest-winston": "^1.9.4",
    "nestjs-prisma": "^0.20.0",
    "nestjs-zod": "^1.2.1",
    "passport": "^0.6.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^4.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "socket.io": "^4.6.1",
    "tozod": "^3.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
